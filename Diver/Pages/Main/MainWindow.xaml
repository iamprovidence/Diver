<Window x:Class="Diver.Pages.Main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diver.Pages.Main"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid>
        
        <ListBox ItemsSource="{Binding Images}">
                        
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel Margin="5">
                        <Label>ImageId</Label> <TextBlock FontSize="9" Text="{Binding Path=ImageId}" />
                        <Label>Repository</Label> <TextBlock Text="{Binding Path=Repository}" />
                        <Label>Tag</Label> <TextBlock Text="{Binding Path=Tag}" />
                    </StackPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
         
    </Grid>
</Window>
